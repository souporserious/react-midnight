{
  "name": "react-midnight",
  "version": "0.7.0",
  "description": "Handle dates, times, and calendars with React.",
  "main": "lib/react-dately.js",
  "scripts": {
    "build": "npm run build:lib && NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:lib": "babel src --out-dir lib",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build",
    "postbuild": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/react-dately"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "calendar",
    "date",
    "date-picker",
    "datepicker",
    "time",
    "time-picker",
    "timepicker",
    "picker"
  ],
  "author": "Travis Arnold <travis@souporserious.com> (http://souporserious.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/souporserious/react-dately/issues"
  },
  "homepage": "https://github.com/souporserious/react-dately",
  "peerDependencies": {
    "react": ">0.13.0",
    "react-dom": ">0.13.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "css-loader": "^0.15.1",
    "expose-loader": "^0.7.1",
    "fbjs": "^0.2.1",
    "http-server": "^0.8.0",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react": "^15.0.2",
    "react-addons-perf": "^15.0.2",
    "react-dom": "^15.0.2",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  }
}
